/dts-v1/;
#include "exynos3250.dtsi"
/ {
	compatible = "samsung,artik5", "samsung,exynos3250", "samsung,exynos3";

    aliases {
    	console = &uart2;
    };

    chosen {
        stdout-path = "/serial@13820000:115200n8";
    };

	memory@40000000 {
		device_type = "memory";
		reg = <0x40000000 0x1f800000>;
		u-boot,dm-pre-reloc;
	};

};
 
&fin_pll {
	clock-frequency = <24000000>;
};

